{
  "parser"  : "babel-eslint",
  "extends" : [
    "standard",
    "standard-react"
  ],
  "env"     : {
    "browser" : true
  },
  "rules": {
    "semi" : [2, "never"],
    "max-len": [2, 160, 2],
    "react/jsx-no-bind": [1, {"allowArrowFunctions": true}],
    "react/no-unused-prop-types": 0
  }
}
